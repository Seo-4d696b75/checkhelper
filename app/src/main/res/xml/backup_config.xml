<?xml version="1.0" encoding="utf-8"?>
<full-backup-content xmlns:tools="http://schemas.android.com/tools">
    <include
        domain="sharedpref"
        path="." />
    <exclude
        domain="sharedpref"
        path="permission_datastore.xml" />
    <exclude
        domain="external"
        path="."
        tools:ignore="FullBackupContent" />
    <exclude
        domain="file"
        path="."
        tools:ignore="FullBackupContent" />
    <exclude
        domain="database"
        path="."
        tools:ignore="FullBackupContent" />
</full-backup-content>
